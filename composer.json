{
    "name": "ecentria/core-rest-bundle",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "description": "Core REST bundle that all Ecentria REST services will extend.",
    "keywords": ["ecentria", "library", "rest", "core"],
    "homepage": "http://stash.dev.opticplanet.net/projects/LIB/repos/corerestbundle/browse",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick McGovern",
            "email": "patrick.mcgovern@opticsplanet.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://bundles.srv-dev-60.usgroup.loc"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "friendsofsymfony/rest-bundle": "1.3.*",
        "jms/serializer-bundle": "0.13.*",
        "willdurand/rest-extra-bundle": "@stable"
    },
    "require-dev": {
        "symfony/framework-bundle": ">=2.1,<2.2-dev"
    },
    "autoload": {
        "psr-4": { "Ecentria\\Libraries\\CoreRestBundle\\": "" }
    },
    "branch-alias": {
        "dev-master": "0.0.x-dev"
    }
}
